@model IEnumerable<VolvoCleaner.Web.Frontend.Models.FileModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Upload">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OriginalUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProcessedUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LogUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    <a asp-action="Process" asp-route-id="@item.Id"> @Html.DisplayFor(modelItem => item.OriginalUrl)</a>
                </td>
                <td>
                    <a asp-action="Process" asp-route-id="@item.Id">@Html.DisplayFor(modelItem => item.ProcessedUrl)</a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LogUrl)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedDate)
                </td>

            </tr>
        }
    </tbody>
</table>
